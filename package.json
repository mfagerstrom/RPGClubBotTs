{
  "name": "rpgclubbot_ts",
  "version": "1.3.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "rm -r build | tsc",
    "bs": "tsc | node build/main.js",
    "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
    "start": "node build/main.js",
    "start:prod": "pm2 start build/main.js",
    "watch": "nodemon --exec npm run dev --watch src --ext ts"
  },
  "dependencies": {
    "@discordx/importer": "^1.3.1",
    "@discordx/pagination": "^3.5.4",
    "axios": "^1.7.7",
    "cheerio": "^1.0.0",
    "discord.js": "^14.15.3",
    "discordx": "^11.11.1",
    "fast-levenshtein": "^3.0.0",
    "pm2": "^5.4.2",
    "user-agents": "^1.1.325"
  },
  "devDependencies": {
    "@types/fast-levenshtein": "^0.0.4",
    "@types/mssql": "^9.1.5",
    "@types/node": "^20.14.4",
    "@types/user-agents": "^1.0.4",
    "dotenv": "^16.4.5",
    "mssql": "^11.0.1",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
